    {
      "AG-add": {
        "api": "/AGV8",
        "type": "entity",
        "formclass": "row",
        "title": "Add new AG",
        "fieldsets": [
          {
            "definition": "AGV8",
            "dependencies": {
              "mobileAvailablity": {
                "oneOf": [
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["हाँ"]
                      },
                      "AGWhatsappNumber": {
                        "type": "string",
                        "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                        "validation": {
                          "pattern": "[6-9]{1}[0-9]{9}",
                          "message": "MOBILE_NO_MUST_10_DIGIT"
                        }
                      }
                    }
                  },
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["Yes"]
                      }
                    },
                    "required": ["AGWhatsappNumber"]
                  }
                ]
              },
              "mobileAvailablity": {
                "oneOf": [
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["हाँ"]
                      },
                      "AGWhatsappNumber": {
                        "type": "string",
                        "title": "कृपया किशोरी के पास उपलब्ध फ़ोन का नंबर दर्ज करें",
                        "validation": {
                          "pattern": "[6-9]{1}[0-9]{9}",
                          "message": "MOBILE_NO_MUST_10_DIGIT"
                        }
                      }
                    }
                  },
                  {
                    "properties": {
                      "mobileAvailablity": {
                        "enum": ["Yes"]
                      }
                    },
                    "required": ["AGWhatsappNumber"]
                  }
                ]
              }
            },
            "fields": [
              {
                "name": "AGfullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "NAME_NOT_VALID"
                }
              },
              {
                "name": "dob",
                "required": true,
                "type": "date",
                "format": "dd/mm/yyyy",
                "validation": {
                  "max": "2008-11-15",
                  "message": "AGE_SHOULD_BE_GREATER_THAN_14"
                }
              },
              {
                "name": "AgAddress",
                "children": {
                  "definition": "Address",
                  "title": false,
                  "fields": [
                    {
                      "name": "district",
                      "dependent": {
                        "key": "district",
                        "title": "District",
                        "type": "select",
                        "options": "this.locationService.getDistrict()"
                      }
                    },
                    {
                      "name": "block",
                      "dependentOn": {
                        "key": "block",
                        "title": "Block",
                        "dependentOn": "district",
                        "type": "select",
                        "options": []
                      }
                    },
                    {
                      "name": "village",
                      "dependentOn": {
                        "key": "village",
                        "title": "Village",
                        "dependentOn": "block",
                        "type": "select",
                        "options": []
                      }
                    }
                  ]
                }
              },
              {
                "name": "PanchayatList"
              },
              {
                "name": "maritalStatus"
              },
              {
                "name": "category",
                "required": true
              },
              {
                "name": "connectVia",
                "required": true
              },
              {
                "name": "fatherFullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "NAME_NOT_VALID."
                }
              },
              {
                "name": "motherFullName",
                "required": true,
                "validation": {
                  "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                  "message": "NAME_NOT_VALID."
                }
              },
              {
                "name": "parentsMobileNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "MOBILE_NO_MUST_10_DIGIT"
                }
              },
              {
                "name": "sameAsAbove",
                "custom": true,
                "element": {
                  "title": "Same as above?",
                  "widget": {
                    "formlyConfig": {
                      "type": "checkbox",
                      "fieldGroupClassName": "controls",
                      "className": "checkbox",
                      "templateOptions": {}
                    }
                  }
                }
              },
              {
                "name": "parentsWhatsappNumber",
                "required": true,
                "validation": {
                  "pattern": "[6-9]{1}[0-9]{9}",
                  "message": "MOBILE_NO_MUST_10_DIGIT"
                },
                "expressionProperties": {
                  "hideExpression": "model.sameAsAbove",
                  "templateOptions.disabled": "model.sameAsAbove",
                  "templateOptions.required": "!model.sameAsAbove"
                }
              },
              {
                "name": "mobileAvailablity",
                "required": true
              },

              {
                "name": "lastStandardOfEducation",
                "required": true
              },
              {
                "name": "lastStandardOfEducationYear",
                "required": true,
                "type": "date",
                "format": "dd/mm/yyyy",
                "validation": {
                  "future": false
                }
              },
              {
                "name": "reasonOfLeavingEducation",
                "required": true
              },
              {
                "name": "whereStudiedLast",
                "required": true
              },
              {
                "name": "registrationStatus",
                "required": true
              },
              {
                "name": "AGDocumentsV3",
                "required": true,
                "title": "नीचे दी गई सूची से कृपया उन दस्तावेजों का चयन करें जो किशोरी के पास उपलब्ध हैं*",
                "type": "custom:document",
                "expressionProperties": {
                  "templateOptions.disabled": "model.whereStudiedLast",
                  "templateOptions.required": "!model.whereStudiedLast"
                }
              },
              {
                "name": "aadharNumber",
                "required": true,
                "validation": {
                  "pattern": "^[0-9]{12}$",
                  "message": "AADHAR_NOT_VALID."
                }
              }
            ]
          }
        ],
        "redirectTo": "/profile/myags"
      }
