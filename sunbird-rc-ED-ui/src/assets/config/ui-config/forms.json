                }
              },
              {
                "name": "geoLocation",
                "required": true

              },
              {
                "name": "campAddress",
                "required": true
              },
              {
                "name": "locationType",
                "required": true

              },
              {
                "name": "contactOfPersonWhoseHomeUsed",
                "required": true

              },
              {
                "name": "CampOwnerDetails",
                "children": {
                  "definition": "CampOwnerDetails",
