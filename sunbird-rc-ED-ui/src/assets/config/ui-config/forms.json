                  "fields": [
                    {
                      "name": "campName",
                      "required": true,
                      "validation": {
                        "pattern": "^[a-zA-Z]+ [a-zA-Z]+$",
                        "message": "Name is not valid."
                      }
                    },
                    {
                      "name": "mobileNumber",
                      "required": true,
                      "validation": {
                        "pattern": "[6-9]{1}[0-9]{9}",
                        "message": "MOBILE NUMBER MUST BE 10 DIGIT"
                      }
                    },
                    {
                      "name": "natureOfLocation",
                      "required": true
                    }
                  ]
                }

              },


              {
                "name": "amenities",
                "required": true

